theta <- 1
n <- 1000
samps <- replicate(n = 1000, {
x <- rexp(n, rate = 1)
y <- rexp(n, rate = 1)
mle <- (sum(x) / sum(y)) ^ .5
I_plug_in <- 2 * n / mle ^ 4
I_obs <- 2 * n * sum(x) / mle ^ 3
c(
((I_plug_in) ^ .5) * (mle - theta),
((I_obs) ^ .5) * (mle - theta)
)
})
